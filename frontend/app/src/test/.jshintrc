{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/
  "browser" : true,
  "eqnull"  : true,
  "esnext"  : true,
  "newcap"  : false,

  "globals": {
    "angular":  false,
    
    // JASMINE
    "after"           : false,
    "afterEach"       : false,
    "before"          : false,
    "beforeEach"      : false,
    "describe"        : false,
    "expect"          : false,
    "getJSONFixture"  : false,
    "inject"          : false,
    "it"              : false,
    "jasmine"         : false,
    "module"          : false,
    "spyOn"           : false
  },

  "quotmark"      : true,
  "smarttabs"     : true,
  "sub"           : true,
  "trailing"      : true,
  "undef"         : true,
  "unused"        : true
}
